generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @db.VarChar(255)
  name     String @db.VarChar(255)
  password String @db.VarChar(45)

  @@map("users")
}
